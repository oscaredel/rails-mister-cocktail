<div class="container">
  <h1>All cocktails <small>(<%= @cocktails.count %>)</small></h1>
  <br>
  <div class="card-deck">
    <% @cocktails.each do |cocktail| %>
      <div class="card">
        <div class="card-body">
          <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
          <h5 class="card-title">
            <%= cocktail.name %>
          </h5>
          <%= link_to "ingredients", cocktail_path(cocktail), class:'card-link'%>
        </div>
      </div>
    <% end %>
  </div>
</div>
